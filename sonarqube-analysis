node {

 stage('Compile-Package'){
      // Get maven home path
      def mvnHome =  tool name: 'maven3.3.9', type: 'maven'  
      sh "${mvnHome}/bin/mvn package"
   }
   
   stage('SonarQube Analysis') {
     def mvnHome =  tool name: 'maven3.3.9', type: 'maven' 
        withSonarQubeEnv('sonarqube') { 
          sh "${mvnHome}/bin/mvn sonar:sonar"
        }
    }
  }
